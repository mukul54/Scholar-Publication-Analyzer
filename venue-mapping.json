            "computer vision and pattern recognition",
            "cvpr",
            "cvf.*?computer vision and pattern",
            "proceedings.*?cvpr",
            "ieee.*?cvf.*?computer vision and pattern recognition",
            "ieee.*?computer.*?society.*?conference.*?computer vision and pattern",
            "proceedings.*?ieee.*?conference.*?computer vision and pattern",
