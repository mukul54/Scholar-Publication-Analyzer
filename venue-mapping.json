  "workshop_detection": {
    "patterns": ["workshop", "ws\\b"],
    "suffix": " Workshop"
  },
  "preprocessing": {
    "remove_patterns": [
      "\\s*,\\s*\\d{4}(\\s|$)",
      "\\s*\\d{4}\\s*$",
      "^\\d{4}\\s+",
      "\\s*,\\s*\\d+(\\s*\\(\\d+\\))?(\\s|$)",
      "\\s*,\\s*pp?\\s*[\\d-]+",
      "\\s*,\\s*\\d+-\\d+\\s*$",
      "^(proceedings of the |proceedings of |proceedings )",
      "\\s+(proceedings|proc\\.?)$",
      "\\s+\\d{4}$",
      "\\s+\\d+$"
    ],
    "skip_short_venues": 3,
    "skip_generic_terms": [
      "the",
      "a",
      "an",
      "in",
      "on",
      "of",
      "and",
      "for",
      "with"
    ]
  }
